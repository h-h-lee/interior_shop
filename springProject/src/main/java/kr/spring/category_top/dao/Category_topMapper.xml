<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="kr.spring.category_top.dao.Category_topMapper">  
  	<update id="updateCategory_top" parameterType="category_topVO">
		UPDATE category_top
		SET 
		  c_top_name = #{c_top_name}
		WHERE c_top_no = #{c_top_no}		  	   
	</update>
</mapper>